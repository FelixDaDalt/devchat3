@using System.Security.Claims
@model List<RoomResponse>
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-md-8 offset-2">
        <h2>Sala de Chat</h2>
        <ul class="list-group">
            @*Aca traigo la lista de chats grupales y la muestro*@
            @foreach(var e in Model)
            {
                <li class="list-group-item "><a href="~/Chat/Room?room=@e.Id&category=@e.CategoryId&senderId=@User.FindFirstValue(ClaimTypes.NameIdentifier)&receiverName=@e.Name">@e.Name</a></li>
            }
        </ul>
    </div>
</div>
